{
    "name": "Table Scraper",
    "version": "1.2",
    "manifest_version": 3,
    "permissions": ["activeTab", "storage", "scripting"],
    "host_permissions": [
        "https://*.finviz.com/*"
     ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://*.finviz.com/*"],
            "js": ["contentScript.js"]
        }
    ]
}
