{
    "name": "Table Scraper",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "storage", "scripting"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
}
